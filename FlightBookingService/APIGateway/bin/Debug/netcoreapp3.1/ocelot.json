      "DownstreamScheme": "http",
      "ServiceName": "inventoryService",
      "UpstreamPathTemplate": "/api/gatway/Flight/GetAllFlightList",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "UseServiceDiscovery": true,
      "DownstreamPathTemplate": "/api/product/findall",
